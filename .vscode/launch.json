{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "start:debug"],
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outputCapture": "console",
      "env": {
        "NODE_ENV": "development",
        "IS_LOCALHOST": "true"
      }
    }
  ]
}
